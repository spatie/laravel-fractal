{
    "name": "spatie/laravel-fractal",
    "description": "An easy to use Fractal integration for Laravel applications",
    "keywords": [
        "spatie",
        "laravel-fractal",
        "laravel",
        "fractal",
        "lumen",
        "api",
        "transform"
    ],
    "homepage": "https://github.com/spatie/laravel-fractal",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "league/fractal": "^0.20.1|^0.20",
        "nesbot/carbon": "^2.63",
        "spatie/fractalistic": "^2.9.5|^2.9",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Fractal\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Fractal\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Fractal\\FractalServiceProvider"
            ],
            "aliases": {
                "Fractal": "Spatie\\Fractal\\Facades\\Fractal"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
