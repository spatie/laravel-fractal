{
    "name": "spatie/laravel-fractal",
    "description": "A Fractal service provider for Laravel 5",
    "keywords": [
        "spatie",
        "laravel-fractal",
        "laravel",
        "fractal",
        "lumen",
        "api",
        "transform"
    ],
    "homepage": "https://github.com/spatie/laravel-fractal",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "league/fractal": "^0.13.0",
        "illuminate/support": "~5.1.0|~5.2.0|~5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0",
        "scrutinizer/ocular": "^1.1",
        "orchestra/testbench": "3.3.x-dev",
        "orchestra/database": "3.3.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Fractal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Fractal\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
